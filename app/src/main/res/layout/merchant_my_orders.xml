<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/white"
    android:orientation="vertical" >



    
    <ListView 
        android:id="@+id/list_myOrders"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:background="@color/white"
        android:cacheColorHint="@android:color/transparent"></ListView>
    <in.pjitsol.zapnabit.Ui.CustomTextView
        android:id="@+id/txt_rcvorder"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="100dp"
        android:padding="20dp"
        app:fontName="GothamLight"
        android:visibility="invisible"
        android:gravity="center"
        android:background="@color/white"
        android:text="@string/recvorder"/>
    

</RelativeLayout>
